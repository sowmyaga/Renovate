  "scripts": {
    "copy-redirect": "cp -r redirect/ build/",
    "prepare": "husky install",
    "prepare-build": "./scripts/prepare-build && node ./scripts/starccmviewer-react-scripts.js",
    "prebuild-for-docker": "npm run prepare-build",
    "prebuild-starccmviewer": "npm run prepare-build",
    "prebuild-starccmauthoring": "npm run prepare-build",
    "prebuild-starccmsteering": "npm run prepare-build",
    "build-for-docker": "cross-env-file -p .env.docker.json vite build --base=__PUBLIC_URL_PLACEHOLDER__ && npm run copy-redirect",
    "build-starccmviewer": "cross-env-file -p .env.starccmviewer.json vite build --base=__PUBLIC_URL_PLACEHOLDER__ && npm run copy-redirect",
    "build-starccmauthoring": "cross-env-file -p .env.starccmauthoring.json vite build --base=__PUBLIC_URL_PLACEHOLDER__ && npm run copy-redirect",
    "build-starccmsteering": "cross-env-file -p .env.starccmsteering.json vite build --base=__PUBLIC_URL_PLACEHOLDER__ && npm run copy-redirect",
    "prestart-starccmviewer": "npm run prepare-build",
    "prestart-starccmauthoring": "npm run prepare-build",
    "prestart-starccmsteering": "npm run prepare-build",
    "start-starccmviewer": "cross-env-file -p .env.starccmviewer.json npm run start",
    "start-starccmviewer:prod": "npm run build-starccmviewer && ./scripts/process-build && npx http-server build",
    "start-starccmauthoring": "cross-env-file -p .env.starccmauthoring.json npm run start",
    "start-starccmsteering": "cross-env-file -p .env.starccmsteering.json npm run start",
    "lint:js": "eslint --max-warnings=0 ./src ./tests",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:styles": "stylelint './src/**/*.tsx'",
    "lint:styles:fix": "stylelint --fix './src/**/*.tsx'",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:fix": "npm run lint:styles:fix && npm run lint:js:fix",
    "build": "cross-env PUBLIC_URL=/$(./scripts/get-public-url) vite build",
    "start": "vite",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "codegen": "graphql-codegen --config codegen.js",
    "deploy-plugins": "AWS_STACK_NAME=star-view-plus-client ./scripts/deploy -d skeleton,starccmviewer,starccmauthoring,starccmsteering,chips",
    "process-docker-build": "./scripts/process-docker -d skeleton,starccmviewer,starccmauthoring,starccmsteering,chips",
    "promote-plugins": "AWS_STACK_NAME=star-view-plus-client ./scripts/promote -p skeleton,starccmviewer,starccmauthoring,starccmsteering,chips",
    "deploy-starccmviewer": "AWS_STACK_NAME=star-view-plus-client ./scripts/deploy -d starccmviewer",
    "promote-starccmviewer": "AWS_STACK_NAME=star-view-plus-client ./scripts/promote -p starccmviewer",
    "undeploy-starccmviewer": "AWS_STACK_NAME=star-view-plus-client ./scripts/deploy -u",
    "prepare-test": "./scripts/prepare-test-file",
    "test": "node scripts/test.js --watchAll=false",
    "test:coverage": "node scripts/test.js --coverage --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test-e2e-starccmviewer": "./scripts/prepare-test-file && cross-env-file -p .env.starccmviewer.json npx playwright test tests/starccmviewer",
    "test-e2e-starccmauthoring": "cross-env-file -p .env.starccmauthoring.json npx playwright test tests/starccmauthoring",
    "test-e2e-starccmsteering": "cross-env-file -p .env.starccmsteering.json npx playwright test tests/starccmsteering",
    "test-e2e-starccmviewer:debug": "./scripts/prepare-test-file && cross-env-file -p .env.starccmviewer.json npx playwright test tests/starccmviewer --debug",
    "test-e2e-starccmauthoring:debug": "cross-env-file -p .env.starccmauthoring.json npx playwright test tests/starccmauthoring --debug",
    "test-e2e-starccmsteering:debug": "cross-env-file -p .env.starccmsteering.json npx playwright test tests/starccmsteering --debug",
    "test-e2e-starccmviewer:trace": "./scripts/prepare-test-file && cross-env-file -p .env.starccmviewer.json npx playwright test tests/starccmviewer --ui-port 0",
    "test-e2e-starccmauthoring:trace": "cross-env-file -p .env.starccmauthoring.json npx playwright test tests/starccmauthoring --ui-port 0",
    "test-e2e-starccmsteering:trace": "cross-env-file -p .env.starccmsteering.json npx playwright test tests/starccmsteering --ui-port 0",
    "test-e2e-starccmviewer:update-snaps": "./scripts/prepare-test-file && cross-env-file -p .env.starccmviewer.json npx playwright test tests/starccmviewer --update-snapshots",
    "test-e2e-starccmauthoring:update-snaps": "cross-env-file -p .env.starccmauthoring.json npx playwright test tests/starccmauthoring --update-snapshots",
    "test-e2e-starccmsteering:update-snaps": "cross-env-file -p .env.starccmsteering.json npx playwright test tests/starccmsteering --update-snapshots",
    "test-integration": "RUN_INTEGRATION_TESTS=true npm test -- --testMatch='<rootDir>/src/integration-tests/**/*.test.ts?(x)' integration-tests",
    "start-skeleton": "cross-env-file -p .env.skeleton.json npm run start",
    "build-skeleton": "cross-env-file -p .env.skeleton.json vite build --base=__PUBLIC_URL_PLACEHOLDER__ && npm run copy-redirect",
    "test-e2e-skeleton": "cross-env-file -p .env.skeleton.json npx playwright test tests/skeleton",
    "test-e2e-skeleton:trace": "cross-env-file -p .env.skeleton.json npx playwright test tests/skeleton --ui-port 0",
    "test-e2e-skeleton:debug": "cross-env-file -p .env.skeleton.json npx playwright test tests/skeleton --debug",
    "test-e2e-skeleton:update-snaps": "cross-env-file -p .env.skeleton.json npx playwright test tests/skeleton --update-snapshots",
    "start-chips": "cross-env-file -p .env.chips.json npm run start",
    "build-chips": "cross-env-file -p .env.chips.json vite build --base=__PUBLIC_URL_PLACEHOLDER__ && npm run copy-redirect",
    "test-e2e-chips": "cross-env-file -p .env.chips.json npx playwright test tests/chips",
    "test-e2e-chips:trace": "cross-env-file -p .env.chips.json npx playwright test tests/chips --ui-port 0",
    "test-e2e-chips:debug": "cross-env-file -p .env.chips.json npx playwright test tests/chips --debug",
    "test-e2e-chips:update-snaps": "cross-env-file -p .env.chips.json npx playwright test tests/chips --update-snapshots",
    "prepare-storybook": "sass --load-path=node_modules src/scss/index.scss .storybook/index.css",
    "storybook": "npm run prepare-storybook && storybook dev -p 6006",
    "build-storybook": "npm run prepare-storybook && storybook build",
    "test:storybook": "npm run prepare-storybook && test-storybook"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true,
    "coverageThreshold": {
      "./src/star-ccmp-lib/genui/": {
        "branches": 72,
        "functions": 79,
        "lines": 81,
        "statements": 80
      },
      "./src/plugins/starccmauthoring/": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      },
      "./src/plugins/starccmsteering/": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      },
      "./src/plugins/starccmviewer/": {
        "branches": 20,
        "functions": 35,
        "lines": 20,
        "statements": 20
      },
      "./src/plugins/starccmviewer/model/reducer.ts": {
        "branches": 87,
        "functions": 96,
        "lines": 89,
        "statements": 87
      },
      "./src/plugins/chips/": {
        "branches": 35,
        "functions": 30,
        "lines": 30,
        "statements": 29
      },
      "./src/plugins/skeleton/": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.ts",
      "<rootDir>/src/**/*.test.tsx"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/plugins/*/model/index.ts",
      "!src/plugins/*/interface.ts",
      "!src/**/styles.ts"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@webxr-input-profiles/motion-controllers)/",
      "^.+\\.module\\.(css|sass|scss)$",
      "!src/plugins/starccmviewer/component/__tests__/VRSession.test.ts"
    ]
  },
